  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "bulma": "^0.7.5",
    "firebase": "^6.5.0",
    "flatpickr": "^4.6.2",
    "register-service-worker": "^1.6.2",
    "vee-validate": "^2.2.15",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-click-outside": "^1.0.7",
    "vue-file-agent": "^1.1.2",
    "vue-property-decorator": "^8.1.1",
    "vue-router": "^3.1.3",
    "vuefire": "^2.1.2",
    "vuex": "^3.1.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@types/dotenv": "^6.1.1",
    "@vue/cli-plugin-babel": "^4.0.0-alpha.3",
    "@vue/cli-plugin-pwa": "^4.0.0-alpha.3",
    "@vue/cli-plugin-typescript": "^4.0.0-alpha.3",
    "@vue/cli-service": "^4.0.0-alpha.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "colors": "^1.3.3",
    "firebase-tools": "^7.3.0",
    "lint-staged": "^9.2.5",
    "sass": "^1.21.10",
    "sass-loader": "^8.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
